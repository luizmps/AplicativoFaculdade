<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/nav_home">
    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.faculdadefacil.DashboardFragment"
        android:label="Home"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_nav_home_to_nav_carterinha"
            app:destination="@id/nav_carterinha" />
    </fragment>
    <fragment
        android:id="@+id/nav_carterinha"
        android:name="com.example.faculdadefacil.CarteirinhaFragment"
        android:label="Carterinha"
        tools:layout="@layout/fragment_carteirinha" />
</navigation>